<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ControllerReg</servlet-name>
        <servlet-class>JavaServletCode.ControllerReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerLogin</servlet-name>
        <servlet-class>JavaServletCode.ControllerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerReg2</servlet-name>
        <servlet-class>JavaServletCode.ControllerReg2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerLogin2</servlet-name>
        <servlet-class>JavaServletCode.ControllerLogin2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerDelete</servlet-name>
        <servlet-class>JavaServletCode.ControllerDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerReg</servlet-name>
        <url-pattern>/ControllerReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerLogin</servlet-name>
        <url-pattern>/ControllerLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerReg2</servlet-name>
        <url-pattern>/ControllerReg2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerLogin2</servlet-name>
        <url-pattern>/ControllerLogin2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerDelete</servlet-name>
        <url-pattern>/ControllerDelete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
